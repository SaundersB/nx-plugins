{
  "npmScope": "otterdev",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "nx-cdk": {
      "tags": []
    },
    "nx-cdk-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-cdk"]
    },
    "nx-exec": {
      "tags": []
    },
    "nx-exec-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-exec"]
    },
    "nx-sst": {
      "tags": []
    },
    "nx-sst-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-sst"]
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  }
}
