{
  "$schema": "http://json-schema.org/schema",
  "executors": {
    "bootstrap": {
      "implementation": "./src/executors/bootstrap/executor",
      "schema": "./src/executors/bootstrap/schema.json",
      "description": "execute cdk bootstrap"
    },
    "stack": {
      "implementation": "./src/executors/stack/executor",
      "schema": "./src/executors/stack/schema.json",
      "description": "execute cdk for given stacks"
    },
    "synth": {
      "implementation": "./src/executors/synth/executor",
      "schema": "./src/executors/synth/schema.json",
      "description": "execute cdk synth"
    },
    "deploy": {
      "implementation": "./src/executors/deploy/executor",
      "schema": "./src/executors/deploy/schema.json",
      "description": "execute cdk deploy"
    },
    "destroy": {
      "implementation": "./src/executors/destroy/executor",
      "schema": "./src/executors/destroy/schema.json",
      "description": "execute cdk destroy"
    }
  }
}
